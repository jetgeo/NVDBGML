            <annotation>
              <documentation>type veg (FormOfWay).</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="veglenkeadresse" type="vegnett:VeglenkeadressePropertyType">
            <annotation>
              <documentation>sammensatt identifikator for veglenkeadresse.
Merknad: Komplett vegadresse består i tillegg av husnummer og bokstav</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="vegreferanse" type="vegnett:VegreferansePropertyType">
            <annotation>
              <documentation>sammensatt identifikator for vegreferanse</documentation>
              <appinfo>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">VREF</taggedValue>
              </appinfo>
            </annotation>
          </element>

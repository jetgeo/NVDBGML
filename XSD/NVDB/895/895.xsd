  <simpleType name="02_BruddårsakEnumerationType">
    <annotation>
      <documentation>10749: Angir hva som er årsak til bruddhendelse</documentation>
      <appinfo>
        <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="NVDB_ID">10749</taggedValue>
